{
  "name": "git-stash-remote",
  "version": "0.0.1",
  "description": "TODO",
  "bin": {
    "stash-remote": "cli.js"
  },
  "main": "index.js",
  "scripts": {
    "test": "xo && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ti0ma/git-stash-remote.git"
  },
  "author": "ti0ma",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ti0ma/git-stash-remote/issues"
  },
  "homepage": "https://github.com/ti0ma/git-stash-remote#readme",
  "dependencies": {
    "co": "^4.6.0",
    "execa": "^0.7.0",
    "project-name": "^0.2.6"
  },
  "devDependencies": {
    "ava": "^0.20.0",
    "xo": "^0.18.2"
  },
  "ava": {
    "files": [
      "**/*.spec.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "failWithoutAssertions": false,
    "powerAssert": false
  },
  "xo": {
    "space": true,
    "rules": {
      "no-unused-vars": [
        "error",
        {
          "ignoreRestSiblings": true,
          "args": "none"
        }
      ],
      "no-warning-comments": [
        0
      ],
      "arrow-parens": [
        "error",
        "always"
      ],
      "object-shorthand": [
        "off"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ]
    },
    "settings": {
      "import/resolver": {
        "node": {
          "paths": [
            "./"
          ]
        }
      }
    }
  }
}
